{
  "name": "@wdio/chrome-recorder",
  "version": "0.5.1",
  "description": "Generate WebdriverIO Tests from Google Chrome DevTools Recordings.",
  "main": "dist/index.js",
  "bin": "bin/wdio-chrome-recorder.js",
  "files": [
    "bin",
    "dist"
  ],
  "type": "module",
  "scripts": {
    "build": "tsc -p .",
    "clean": "rm -rf dist",
    "test": "run-s test:*",
    "test:lint": "eslint '**/*.{js,ts}' --fix .",
    "test:unit": "vitest",
    "watch": "tsc -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webdriverio/chrome-recorder.git"
  },
  "keywords": [
    "webdriverio",
    "wdio",
    "testing",
    "e2e",
    "automation",
    "devtools"
  ],
  "author": "Christian Bromann <mail@bromann.dev>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/webdriverio/chrome-recorder/issues"
  },
  "homepage": "https://github.com/webdriverio/chrome-recorder#readme",
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/inquirer": "^9.0.3",
    "@types/node": "^18.11.9",
    "@types/prettier": "^2.7.1",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "@vitest/coverage-c8": "^0.25.1",
    "chai": "^4.3.7",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "npm-run-all": "^4.1.5",
    "release-it": "^15.5.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "vitest": "^0.25.1"
  },
  "dependencies": {
    "@puppeteer/replay": "^2.2.0",
    "chalk": "^5.1.2",
    "globby": "^13.1.2",
    "inquirer": "^9.1.4",
    "meow": "^11.0.0",
    "prettier": "^2.7.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
